  <p class="price"><?= $this->convertedCurrentPrice ?>€ (<?= $this->condition ?>)</p>
  <p class="end-time"><?= $this->endTime ?></p>
  <p class="shipping"><?= $this->shipToLocations ?> <?= $this->shippingType ?> <?= $this->shippingCosts ?>€</p>
